@model ZaculeuValley.IxchelAdmin.Models.Institution

@{
    ViewData["Title"] = "Details";
}

<h1>Detalles</h1>

<div>
    <h4>Institución</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Nombre Institución
            @*@Html.DisplayNameFor(model => model.InstitutionName)*@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InstitutionName)
        </dd>
        <dt class = "col-sm-2">
            Código Institución
            @*@Html.DisplayNameFor(model => model.InstitutionCode)*@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InstitutionCode)
        </dd>
        <dt class = "col-sm-2">
            Hábilitado
            @*@Html.DisplayNameFor(model => model.Enabled)*@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Enabled)
        </dd>
        <dt class = "col-sm-2">
            Borrado
            @*@Html.DisplayNameFor(model => model.Deleted)*@
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Deleted)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Idinstitution">Editar</a> |
    <a asp-action="Index">Regresar a Índice</a>
</div>
